<Pokemons>
  <Pokemon id="1" name="Bulbasaur">
    <Type>Grass</Type>
    <Type2>Poison</Type2>
    <Species>Seed Pokémon</Species>
    <Description>For some time after its birth, it grows by gaining nourishment from the seed on its back.</Description>
    <Evolution level="16">Ivysaur</Evolution>
  </Pokemon>
  <Pokemon id="2" name="Ivysaur">
    <Type>Grass</Type>
    <Type2>Poison</Type2>
    <Species>Seed Pokémon</Species>
    <Description>When the bud on its back starts swelling, a sweet aroma wafts to indicate the flowers coming bloom.</Description>
    <Evolution level="32">Venusaur</Evolution>
  </Pokemon>
  <Pokemon id="3" name="Venusaur">
    <Type>Grass</Type>
    <Type2>Poison</Type2>
    <Species>Seed Pokémon</Species>
